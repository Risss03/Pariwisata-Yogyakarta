<form method="post" action="/admin/pariwisata" enctype="multipart/form-data">
    <div class="form-group">
        <label for="judul">Judul</label>
        <input type="text" class="form-control" id="judul" name="judul" required>
    </div>
    <div class="form-group">
        <label for="deskripsi">Deskripsi</label>
        <textarea class="form-control" id="deskripsi" name="deskripsi" rows="3" required></textarea>
    </div>
    <div class="form-group">
        <label for="gambar">Gambar</label>
        <input type="file" class="form-control-file" id="gambar" name="gambar" required>
    </div>
    <div class="form-group">
        <label for="deskripsiGambar">Deskripsi Gambar</label>
        <textarea class="form-control" id="deskripsiGambar" name="deskripsiGambar" rows="3" required></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Tambah Pariwisata</button>
</form>
